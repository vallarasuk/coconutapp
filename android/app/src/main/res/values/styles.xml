<resources>
  <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="android:textColor">@android:color/black</item>
    <item name="android:editTextStyle">@style/ResetEditText</item>
    <item name="android:editTextBackground">@drawable/rn_edit_text_material</item>
  </style>
  <style name="ResetEditText" parent="@android:style/Widget.EditText">
    <item name="android:padding">0dp</item>
    <item name="android:textColorHint">#c8c8c8</item>
    <item name="android:textColor">@android:color/black</item>
  </style>
  <style name="Theme.App.thidiff" parent="AppTheme">
    <item name="android:windowBackground">@drawable/thidiff</item>
 </style>
  <style name="Theme.App.zunomart" parent="AppTheme">
    <item name="android:windowBackground">@drawable/zunomart</item>
 </style>
   <style name="Theme.App.zunomartstore" parent="AppTheme">
    <item name="android:windowBackground">@drawable/zunomartstore</item>
 </style>
  <style name="Theme.App.zunostar" parent="AppTheme">
    <item name="android:windowBackground">@drawable/zunostar</item>
 </style>
</resources>


